            <kind>1</kind>
            <string>///Game Over
if( global.life &lt;=0 || global.shoots == 0 || global.lostDinos &gt;= 500){
    stateGame = "gameOver";
    //audio_sound_gain(audio_play_sound( overDino,1,false),1,0)
    if !audio_is_playing( overDino){
        audio_stop_all();
        audio_play_sound( overDino,1,false);
    }
    global.lostDinos = 500;//mantiene el numero maximo de dinos perdidos
    with( o_rocky) instance_destroy();
    with( o_turtleBall) instance_destroy();
    with( o_pause) instance_destroy();
    
    //Menu game over
    if( keyboard_check_pressed( vk_down) &amp;&amp; option == 1){
        option++;
        selectorY = posY + 70;
    }
    if( keyboard_check_pressed( vk_up) &amp;&amp; option == 2){
        option--;
        selectorY = posY;
    }
    
    if( keyboard_check_pressed( vk_enter)){
        if( option == 1){
            game_restart();
            audio_stop_all();
        }
        else{
            game_end();
        }
    }
}
